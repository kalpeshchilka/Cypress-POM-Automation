{
  "name": "Cypress-POM-Automation",
  "version": "1.0.0",
  "description": "This Project automates SauceDemo.com website using Cypress.io and Page Object Model concepts",
  "main": "index.js",
  "scripts": {
    "start:ci": "serve --no-clipboard --listen ${PORT:-8080}",
    "cy:verify": "cypress verify",
    "test:headed": "cypress run --browser chrome",
    "test:headless": "cypress run --headless --browser chrome",
    "cy:open": "cypress open",
    "cy:record": "cypress run --record --key b56e02d2-8d54-4e3e-a80d-ad899371c367",
    "cy:record-parallel": "cypress run --record --key b56e02d2-8d54-4e3e-a80d-ad899371c367 --ci-build-id 2 --parallel"
  },
  "author": "kalpesh",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^5.1.0",
    "mocha": "^8.1.3",
    "mochawesome": "^6.1.1"
  }
}